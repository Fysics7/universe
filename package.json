{
  "name": "universe",
  "version": "1.0.0",
  "description": "MERN application that allows users to create a Captain, pick a species, and select a spaceship to explore a galaxy",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "server": "node server.js",
    "client": "node scripts/start-client.js",
    "start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "build": "node scripts/build.js",
    "seed": "node scripts/seedDB.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fysics7/universe.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fysics7/universe/issues"
  },
  "homepage": "https://github.com/Fysics7/universe#readme",
  "dependencies": {
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.3.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4"
  }
}
